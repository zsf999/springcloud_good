seajs.use(["//misc.360buyimg.com/jdf/1.0.0/unit/trimPath/1.0.0/trimPath","//misc.360buyimg.com/jdf/2.0.0/ui/fixable/1.0.0/fixable","//misc.360buyimg.com/jdf/2.0.0/ui/elevator/1.0.0/elevator","//misc.360buyimg.com/jdf/2.0.0/ui/gotop/1.0.0/gotop"],function(a,b,c,d){var e=$(".elevator"),f=e.data("path"),g=window.channelData["floor-"+f],h=e.find('[type="text/trimpath"]').html(),i=window.screen.width;if(1366>=i)e.hide();else{window.screen.width;e.html(h.process(g)).removeClass("channel-lazyfloor"),e.fixable({x:"right",y:"top",xValue:-60,yValue:"center",threshold:600,contextEl:$(".elevator-floor .w")}),$("body").elevator({floorEl:".elevator-floor",elevatorEl:".elevator",handlerSelector:".handler",curCls:"current",threshold:500}),e.find(".go-top").gotop({easing:"swing"})}});